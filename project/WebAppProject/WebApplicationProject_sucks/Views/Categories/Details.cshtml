@model WebApplicationProject_sucks.Models.Category

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

    <h4>Category</h4>
    <hr />

        @{           
            var room = Model.Rooms.GetEnumerator();
            while (room.MoveNext())
            {
            <tr>
            <td>
                @{MyDB db = new MyDB();
                    var title = db.QuestionRooms.Where(d => d.QuestionRoomID == room.Current.QuestionRoomID).ToList().ElementAt(0).Title;
                    string linkToQR = "../../QuestionRooms/Details/" + room.Current.QuestionRoomID;
                }
                <a href=@linkToQR >@Html.Raw(title)</a>
            </td>
       
            </tr>
            }
        }

<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Name }) |
    @Html.ActionLink("Back to List", "Index")
</p>
