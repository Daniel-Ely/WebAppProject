@model IEnumerable<WebApplicationProject_sucks.Models.Category>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th> </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>

            <td>

                @Html.ActionLink("Details", "Details", new { id = item.Name })
            </td>
        </tr>
    }
        
</table>
<a href="../HomePage/FirstTimeInThisSystem">Back to create categories and professions</a>
